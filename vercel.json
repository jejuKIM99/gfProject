{
  "builds": [
    {
      "src": "./vercel-api/krx-gold-fd.py",
      "use": "@vercel/python@3.0.0",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/krx-gold-fd",
      "dest": "/vercel-api/krx-gold-fd.py"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
